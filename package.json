{
  "name": "david-www",
  "version": "16.0.2",
  "description": "Watching your Node.js dependencies.",
  "main": "index.js",
  "homepage": "https://github.com/alanshaw/david-www",
  "repository": {
    "type": "git",
    "url": "git://github.com/alanshaw/david-www.git"
  },
  "bugs": {
    "url": "https://github.com/alanshaw/david-www/issues"
  },
  "keywords": [
    "dependency",
    "management",
    "npm"
  ],
  "author": "Alan Shaw",
  "contributors": [
    "Oliver Evans",
    "XhmikosR"
  ],
  "license": "MIT",
  "scripts": {
    "start": "node dist/main",
    "build": "run-p build:**",
    "build:js:server": "BABEL_ENV=server babel src --out-dir dist",
    "build:js:client": "browserify src/ui/main.jsx -o public/bundle.js",
    "build:css": "node-sass --include-path node_modules src/ui/main.scss public/bundle.css",
    "minify": "run-p minify:*",
    "minify:js": "uglifyjs public/bundle.js -o public/bundle.js",
    "minify:css": "postcss -u cssnano -i public/bundle.css -o public/bundle.css",
    "lint": "standard",
    "test": "run-s lint build test:*",
    "test:unit": "tape test/*.js",
    "watch": "run-p watch:**",
    "watch:js:server": "BABEL_ENV=server babel -w src --out-dir dist",
    "watch:js:client": "watchify src/ui/main.jsx -o public/bundle.js -v",
    "watch:css": "node-sass --include-path node_modules -w src/ui/main.scss public/bundle.css",
    "watch:server": "nodemon --watch dist --watch public -e js,css -x 'npm start'",
    "postinstall": "npm run build && npm run minify"
  },
  "dependencies": {
    "async": "^1.4.1",
    "babel-cli": "^6.8.0",
    "babel-plugin-jsx-strip-ext": "^1.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "boom": "^3.1.3",
    "cheerio": "^0.20.0",
    "compression": "^1.4.1",
    "couchwatch": "^0.6.0",
    "cycle": "^1.0.3",
    "d3": "^3.5.5",
    "david": "^7.0.0",
    "dep-diff": "^1.0.1",
    "express": "^4.13.4",
    "extract": "^1.0.0",
    "font-awesome": "^4.6.1",
    "github": "^1.1.0",
    "github-url": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "level": "^1.3.0",
    "level-session": "^0.3.4",
    "merge": "^1.2.0",
    "meta-marked": "^0.4.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.2",
    "normalize-scss": "^5.0.1",
    "npm": "^3.8.8",
    "nsp-advisories-api": "^2.0.2",
    "rc": "^1.1.5",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-helmet": "^3.1.0",
    "react-modal": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.53.0",
    "rimraf": "^2.4.3",
    "rss": "^1.1.1",
    "semver": "^5.0.1",
    "serve-favicon": "^2.2.0",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "nodemon": "^1.9.2",
    "standard": "^7.1.0",
    "tape": "^4.5.1"
  },
  "engines": {
    "node": "^6.1.0"
  },
  "directories": {
    "test": "test"
  },
  "babel": {
    "env": {
      "server": {
        "presets": [
          "node6",
          "react"
        ],
        "plugins": [
          "jsx-strip-ext"
        ]
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ]
    ]
  },
  "standard": {
    "ignore": [
      "public/js/vendor/*",
      "public/js/plugins.js"
    ]
  }
}
